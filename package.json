{
  "name": "react-bootsrap",
  "version": "0.0.1",
  "description": "React frontend for the Sump Thing project",
  "main": "index.js",
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@material-ui/core": "^4.0.0-beta.1",
    "@material-ui/icons": "^4.0.0-beta.0",
    "@material-ui/styles": "^4.0.0-beta.1",
    "@material-ui/system": "^4.0.0-beta.1",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-runtime": "^6.22.0",
    "connected-react-router": "^6.4.0",
    "css-loader": "2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^3.0.1",
    "happypack": "^5.0.1",
    "history": "^4.9.0",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.8.6",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.4",
    "react-redux": "^7.1.0-alpha.5",
    "react-router-dom": "^5.0.0",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.1",
    "redux-devtools": "^3.5.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "store": "^2.0.12",
    "url-loader": "1.1.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "scripts": {
    "dev": "webpack-dev-server --config config/webpack/main.js --colors",
    "test": "./node_modules/.bin/jest --config='./jest.config.js'",
    "test:watch": "./node_modules/.bin/jest --watch --config='./jest.config.js'",
    "build": "webpack --config config/webpack/production.js"
  },
  "jest": {
    "moduleNameMapper": {
      "^js/(.*)": "<rootDir>/frontend/js/$1",
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/frontend/js/tests/__mocks__/file.js"
    }
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties",
      [
        "@babel/plugin-proposal-decorators",
        {
          "decoratorsBeforeExport": false
        }
      ]
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pyazo/sump-thing-frontend"
  },
  "author": "Pyazo, LLC",
  "license": "ISC"
}
