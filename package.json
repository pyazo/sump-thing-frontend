{
  "name": "react-bootsrap",
  "version": "1.1.0",
  "description": "A foundation for rapid React prototypes",
  "main": "index.js",
  "dependencies": {
    "lodash": "^4.17.4",
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
