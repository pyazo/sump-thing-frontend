    "@babel/preset-stage-2": "^7.0.0",
    "@material-ui/core": "^3.0.2",
    "@material-ui/icons": "^3.0.1",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.2",
    "babel-runtime": "^6.22.0",
    "css-loader": "0.26.1",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "^0.10.0",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "prop-types": "^15.6.0",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^15.6.1",
    "url-loader": "1.1.1",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "scripts": {
    "start": "docker-compose up",
    "stop": "docker-compose down",
    "webpack-dev-server": "webpack-dev-server --config config/webpack/main.js --colors",
    "test": "./node_modules/.bin/jest --config='./jest.config.js'",
    "test:watch": "./node_modules/.bin/jest --watch --config='./jest.config.js'",
    "build": "webpack --config config/webpack/production.js"
  },
  "jest": {
    "moduleNameMapper": {
      "^js/(.*)": "<rootDir>/frontend/js/$1",
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/frontend/js/tests/__mocks__/file.js"
    }
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ]
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "repository": {
